{
  "name": "e-learning-platform",
  "version": "1.0.0",
  "description": "An online learning platform is a webspace or portal for educational content and resources that offers a student everything they need in one place",
  "main": "./src/index.js",
  "scripts": {
    "local": "nodemon -r dotenv/config . dotenv_config_path=./.env.local -L ./index.js",
    "dev": "nodemon -r dotenv/config . dotenv_config_path=./.env.dev ./src/index.js",
    "stage": "node -r dotenv/config . dotenv_config_path=./.env.stage ./src/index.js",
    "start": "node -r dotenv/config . dotenv_config_path=./.env.prod ./src/index.js"
  },
  "keywords": [
    "e-learning",
    "Learning Management System",
    "lms"
  ],
  "author": "Japaha Company Ltd",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.414.0",
    "aws-sdk": "^2.1353.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "express-validator": "^6.15.0",
    "i18n": "^0.15.1",
    "jsonwebtoken": "^8.5.1",
    "mailgun-js": "^0.22.0",
    "mongoose": "^7.2.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^2.7.0",
    "nodemon": "^2.0.20",
    "redis": "^4.6.5",
    "slug": "^8.2.3",
    "socket.io": "^4.7.2"
  }
}
